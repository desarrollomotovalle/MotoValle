                                    <p>
                                        @if (string.IsNullOrEmpty(Model.EngineTypeName))
                                        {
                                            <br />
                                        }
                                        else
                                        {
                                            @Model.EngineTypeName
                                        }
                                    </p>
                                </div>
                                <div class="b-detail__main-info-characteristics-one-bottom">
                                    Motor
                                </div>
                            </div>
                            <div class="b-detail__main-info-characteristics-one massey">
                                <div class="b-detail__main-info-characteristics-one-top">
                                    <div><img src="~/images/custom-icons/capacidad-icono.png" class="fa" width="60%" /></div>
                                    <p>
                                        @if (string.IsNullOrEmpty(Model.GasMileage))
                                        {
                                            <br />
                                        }
                                        else
                                        {
                                            @Model.GasMileage
                                        }
                                    </p>
                                </div>
                                <div class="b-detail__main-info-characteristics-one-bottom">
                                    Capacidad Tanque
                                </div>
                            </div>
                        </div>
                        <div class="b-detail__main-info-text wow zoomInUp" data-wow-delay="0.5s">
