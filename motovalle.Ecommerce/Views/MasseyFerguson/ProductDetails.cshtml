@using Microsoft.AspNetCore.Identity
@using motovalle.Ecommerce.Models.Entities.Identity

@inject UserManager<ApplicationUser> UserManager
@model InventoryAndProductBaseWithDetails

@{
    ViewData["Title"] = "ProductDetails";
    Layout = "~/Views/Shared/_LayoutMasseyFerguson.cshtml";
    var index = 0;
    var userName = string.Empty;
    var userEmail = string.Empty;
    var userPhone = string.Empty;

    if (User.Identity.IsAuthenticated)
    {
        var user = await UserManager.GetUserAsync(User);
        userName = user.FullName;
        userEmail = user.Email;
        userPhone = user.PhoneNumber;
    }
}

<section class="b-pageHeader massey">
    <div class="container">
        <h1 class="wow zoomInLeft" data-wow-delay="0.5s">Detalles del equipo</h1>
    </div>
</section><!--b-pageHeader-->

<div class="b-breadCumbs s-shadow wow zoomInUp" data-wow-delay="0.5s">
    <div class="container">
        <a asp-action="Index" asp-controller="Home" class="b-breadCumbs__page">Home</a><span class="fa fa-angle-right"></span><a asp-action="Index" asp-controller="MasseyFerguson" class="b-breadCumbs__page m-active">Tienda (Massey Ferguson)</a><span class="fa fa-angle-right"></span><a asp-action="ProductInventory" asp-controller="MasseyFerguson" asp-route-productId="@Model?.ProductId" class="b-breadCumbs__page m-active">Producto (Stock)</a><span class="fa fa-angle-right"></span><a asp-action="ProductDetails" asp-controller="MasseyFerguson" asp-route-productId="@Model.ProductId" class="b-breadCumbs__page m-active">Detalles</a>
    </div>
    @if (ViewBag.Message != null)
    {
        <div class="alert alert-danger text-center wow zoomInUp" role="alert" data-wow-delay="0.5s">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <strong>¡Ups!..</strong> @ViewBag.Message &#128577;
        </div>
    }
</div><!--b-breadCumbs-->

<section class="b-detail s-shadow">
    <div class="container">
        <header class="b-detail__head s-lineDownLeft wow zoomInUp" data-wow-delay="0.5s">
            <div class="row">
                <div class="col-lg-9 col-sm-9 col-xs-12">
                    <div class="b-detail__head-title">
                        <h1>@Model.ProductName</h1>
                        <h3>@Model.ProductDescription</h3>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-3 col-xs-12">
                    <div class="b-detail__head-price">
                        <div class="b-detail__head-price-num">@Model.SalesPriceInventory.ToString("C")</div>
                        &nbsp;
                    </div>
                </div>
            </div>
        </header>
        <div class="b-detail__main">

            @if (!string.IsNullOrEmpty(Model.Picture360UrlInventory))
            {
                <div class="row">
                    <div class="col-lg-12 col-sm-12 col-xs-12">

                        <div class="b-detail__main-info">
                            <div class="b-detail__main-info-images wow zoomInUp" data-wow-delay="0.5s">
                                <!--360 -->
                                <div class="text-center">
                                    <h1 class="s-titleDet">Vista 360° Exterior</h1>
                                </div>
                                <div class="img-thumbnail" style="width:100%">
                                    <div class="cloudimage-360"
                                         data-folder="@Url.Content($"~/{Model.Picture360UrlInventory}/")"
                                         data-filename="{index}.jpg"
                                         data-amount="@Model.Picture360QuantityInventory"
                                         data-magnifier="2"
                                         data-spin-reverse>
                                    </div>
                                    <!--360 -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <p>&nbsp;</p>
                </div>
            }
            else
            {
                if (!string.IsNullOrEmpty(Model.Picture360Url))
                {
                    <div class="row">
                        <div class="col-lg-12 col-sm-12 col-xs-12">

                            <div class="b-detail__main-info">
                                <div class="b-detail__main-info-images wow zoomInUp" data-wow-delay="0.5s">
                                    <!--360 -->
                                    <div class="text-center">
                                        <h1 class="s-titleDet">Vista 360° Exterior</h1>
                                    </div>
                                    <div class="img-thumbnail" style="width:100%">
                                        <div class="cloudimage-360"
                                             data-folder="@Url.Content($"~/{Model.Picture360Url}/")"
                                             data-filename="{index}.jpg"
                                             data-amount="@Model.Picture360Quantity"
                                             data-magnifier="2"
                                             data-spin-reverse>
                                        </div>
                                        <!--360 -->
                                    </div>
                                </div>
                            </div>
                        </div>
                        <p>&nbsp;</p>
                    </div>
                }
            }

            @if (!string.IsNullOrEmpty(Model.Cupix360UrlInventory))
            {
                @if (!string.IsNullOrEmpty(Model.Cupix360UrlInventory))
