                                        <h4 class="b-detail__main-aside-desc-title">Número de discos</h4>
                                    </div>
                                    <div class="col-xs-6">
                                        <p class="b-detail__main-aside-desc-value">@Model.NumberOfDiscs</p>
                                    </div>
                                </div>
                            }
                            @if (!string.IsNullOrEmpty(Model.ConfigurationTypeName))
                            {
                                <div class="row">
                                    <div class="col-xs-6">
                                        <h4 class="b-detail__main-aside-desc-title">Configuración</h4>
                                    </div>
                                    <div class="col-xs-6">
                                        <p class="b-detail__main-aside-desc-value">@Model.ConfigurationTypeName</p>
                                    </div>
                                </div>
                            }
                            @if (!string.IsNullOrEmpty(Model.StandardCover))
                            {
                                <div class="row">
                                    <div class="col-xs-6">
                                        <h4 class="b-detail__main-aside-desc-title">Cubierta estándar</h4>
                                    </div>
                                    <div class="col-xs-6">
                                        <p class="b-detail__main-aside-desc-value">@Model.StandardCover</p>
                                    </div>
                                </div>
                            }
                            @if (!string.IsNullOrEmpty(Model.BladeTypeName))
                            {
                                <div class="row">
                                    <div class="col-xs-6">
                                        <h4 class="b-detail__main-aside-desc-title">Cuchillas</h4>
                                    </div>
                                    <div class="col-xs-6">
                                        <p class="b-detail__main-aside-desc-value">@Model.BladeTypeName</p>
                                    </div>
                                </div>
                            }
                            @if (!string.IsNullOrEmpty(Model.TurningRadiusTypeName))
                            {
                                <div class="row">
                                    <div class="col-xs-6">
                                        <h4 class="b-detail__main-aside-desc-title">Radio de giro</h4>
                                    </div>
                                    <div class="col-xs-6">
                                        <p class="b-detail__main-aside-desc-value">@Model.TurningRadiusTypeName</p>
                                    </div>
                                </div>
                            }
                        </div>
                        <div class="b-detail__main-aside-about wow zoomInUp" data-wow-delay="0.5s">
                            <div style="padding-bottom: 2.5em;">
                                <div class="b-search__main-form-submit wow zoomInLeft" data-wow-delay="0.5s" data-wow-offset="100">
                                    <button type="button" class="btn m-btn" onclick="addToCart(@Model.ProductId, @Model.InventoryItemsId, 1)">
                                        Agregar al carrito
                                        <span class="fa fa-shopping-cart"></span>
                                    </button>
                                </div>
                            </div>
                        </div>

                        <div class="b-detail__main-aside-about wow zoomInUp" data-wow-delay="0.5s">
                            <div class="b-detail__main-aside-about-call">
                                @if (User.Identity.IsAuthenticated)
                                {
                                    <span class="fa fa-comments"></span>
                                    <div>
                                        Usa nuestro servicio de chat en línea.
                                        <a onclick="jivo_api.setContactInfo({ name: '@userName', email: '@userEmail', phone: '@userPhone', description: '@Model.ProductName' }); jivo_api.setCustomData({ 'Asunto': 'Quiero saber más sobre: @Model.ProductName' }); jivo_api.open({ start: 'chat' });">Aquí</a>
                                    </div>
                                }
                                else
                                {
                                    <span class="fa fa-comments"></span>
                                    <div>
                                        Usa nuestro servicio de chat en línea.
                                        <a onclick="jivo_api.setCustomData({ 'Asunto': 'Quiero saber más sobre: @Model.ProductName' }); jivo_api.open({ start: 'chat' });">Aquí</a>
                                    </div>
                                }
                            </div>
                            <div class="b-detail__main-aside-about-seller">
                                <p><span>Estámos dispuestos a resolver todas tus dudas</span></p>
                            </div>

                        </div>
                        @{ await Html.RenderPartialAsync("_Leads", new InterestedCustomerViewModel() { FkInventoryItemsId = Model.InventoryItemsId }); }
                    </aside>
                </div>
            </div>
        </div>
    </div>
</section><!--b-detail-->

