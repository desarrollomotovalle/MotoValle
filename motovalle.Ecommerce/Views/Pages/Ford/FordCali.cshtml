    @*Toggle form*@
    <script type="text/javascript">
        function toggleForm() {
            $(".cotizacion-box").animate({
                width: "toggle",
                height: "toggle",
                opacity: "toggle"
            });
        }

        function resizeIframe() {
            const $videoContainer = document.querySelector("#video-container");
            if (document.documentElement.clientWidth < 769) {
                if (!$videoContainer.classList["video-container"]) {
                    $videoContainer.classList.add("video-container");
                }
            } else {
                if ($videoContainer.classList["video-container"]) {
                    $videoContainer.classList.remove("video-container");
                }
            }
        }

        if (document.querySelector(".info-campania")) {
            document.querySelector(".info-campania").addEventListener("click", toggleForm);
        }

        (() => {
            resizeIframe();
        })();

        window.addEventListener('resize', resizeIframe);
    </script>
