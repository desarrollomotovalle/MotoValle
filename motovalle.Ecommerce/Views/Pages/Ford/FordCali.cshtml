                                </div>

                                <div class="text-center">
                                    <input type="submit" class="btn btn-secondary" value="¡Cotiza ya!" />
                                </div>
                            </form>
                            <!-- Form -->
                        </div>
                    </div>
                    <!-- /form user info -->
                </div>
            </div>
        </div>
    </div>
    <!-- End Form -->
</section>
