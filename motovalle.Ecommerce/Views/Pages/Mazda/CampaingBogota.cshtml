@model motovalle.Ecommerce.Models.ViewModels.Landing.ContactViewModel
@{
    ViewData["Title"] = "CampaingSantaFe";
    Layout = "~/Views/Shared/Landing/_LayoutLandingMazda.cshtml";
}

@section Styles{
    <style type="text/css">
        :root {
            --mazdaColor: #A81E30;
            --mazdaColorHover: #8e1928;
        }

        #hero {
            background: url("@Url.Content($"~/landingAssets/landing-mazda/assets/img/campaings/bogota/bogota-mazdalandng.png?v={Guid.NewGuid()}")") top center no-repeat;
            background-size: cover;
            background-repeat: no-repeat;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            height: 100vh;
            width: 100%;
            position: relative;
            border-bottom: 1px solid #222;
            background-color: black
        }

        .cotizacion {
            position: fixed; /* Hacemos que la posición en pantalla sea fija para que siempre se muestre en pantalla*/
            right: 0; /* Establecemos la barra en la izquierda */
            padding: 0;
            top: 200px; /* Bajamos la barra 200px de arriba a abajo */
            z-index: 2000; /* Utilizamos la propiedad z-index para que no se superponga algún otro elemento como sliders, galerías, etc */
        }

        .cotizacion ul {
            list-style: none;
            padding: 0;
            margin: 0;
            writing-mode: vertical-lr;
            text-orientation: sideways-right;
        }

        .cotizacion ul li a {
            display: inline-block;
            color: #fff;
            background: var(--mazdaColor);
            padding: 1.2em 1.2em;
            font-size: 22px;
            text-decoration: none;
            -webkit-transition: all 500ms ease;
            -o-transition: all 500ms ease;
            transition: all 500ms ease; /* Establecemos una transición a todas las propiedades */
        }

        .cotizacion ul li .info-campania {
            background: var(--mazdaColor);
        }

        .cotizacion ul li a:hover.info-campania {
            background: var(--mazdaColorHover); /*Cambiamos el fondo cuando el usuario pase el mouse */
            padding: 1.6em 1.6em; /*Hacemos mas grande el espacio cuando el usuario pase el mouse */
        }

        .cotizacion-box {
            display: none;
            float: left;
            overflow: hidden;
            background: #2E2E2E;
            position: fixed;
            z-index: 2001;
            color: white;
        }

        /* Add padding and border to inner content
        for better animation effect */
        .cotizacion-box-inner {
            width: auto;
            padding: 0px;
        }

        .wpp-contact {
            width: 60%;
        }

        .btn.btn-mazda {
            background-color: var(--mazdaColor);
        }

        .btn.btn-mazda:hover {
            background-color: var(--mazdaColorHover);
        }

        @@media (max-width: 768px) {
            #hero {
                background: url('@Url.Content("~/landingAssets/landing-mazda/assets/img/campaings/bogota/bogota-landing-responsive.png")') top center no-repeat;
                height: 83.3vh;
                background-size: contain;
                background-repeat: no-repeat;
                -webkit-background-size: contain;
                -moz-background-size: contain;
                -o-background-size: contain;
                position: relative;
                background-color: black;
            }

            .cotizacion-box {
                width: 80%;
            }

            .cotizacion ul li a {
                padding: 0.8em 0.8em;
                font-size: 18px;
            }

            .cotizacion ul li a:hover.info-campania {
                background: var(--mazdaColorHover); /*Cambiamos el fondo cuando el usuario pase el mouse */
                padding: 1em 1em; /*Hacemos mas grande el espacio cuando el usuario pase el mouse */
            }

            .card-header > h4 {
                font-size: 18px;
            }

            .wpp-contact {
                width: 140%;
            }

            .custom-size {
                font-size: 12px;
            }
        }

        .video-container {
            overflow: hidden;
            position: relative;
            width: 100%;
        }

        .video-container::after {
            padding-top: 56.25%;
            display: block;
            content: '';
        }

        .video-container iframe {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
        }
    </style>

    <style>
        .wpgmp-map-12, .wpgmp-map-12 .wpgmp_tabs_container, .wpgmp-map-12 .wpgmp_listing_container {
            font-size: 16px !important;
        }
    </style>
}

