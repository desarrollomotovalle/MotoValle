@model string

 <!-- Start navigation pages -->
<nav class="b-nav">
    <div class="container">
        <div class="row">
            <div class="col-sm-3 col-xs-3 wow slideInLeft" data-wow-delay="0.3s">
                <a class="navbar-brand" asp-action="Index" asp-controller="Home"><img src="~/media/brands/motovalle-logo.png" class="img-responsive" alt="Motovalle-logo65"></a>
            </div>
            <div class="col-sm-9 col-xs-9">
                <div class="b-nav__list wow slideInRight" data-wow-delay="0.3s">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#nav">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse navbar-main-slide" id="nav">
                        @switch (Model)
                        {
                            case "Ford":
                                await Html.RenderPartialAsync("~/Views/Shared/NavBars/_FordNavBar.cshtml");
                                break;
                            //case "Mazda":
                            //    await Html.RenderPartialAsync("~/Views/Shared/NavBars/_MazdaNavBar.cshtml");
                            //    break;
                            case "Carshow":
                                await Html.RenderPartialAsync("~/Views/Shared/NavBars/_CarshowNavBar.cshtml");
                                break;
                            case "Massey":
                                await Html.RenderPartialAsync("~/Views/Shared/NavBars/_MasseyNavBar.cshtml");
                                break;
                            case "Repuestos":
                                await Html.RenderPartialAsync("~/Views/Shared/NavBars/_RepuestosNavBar.cshtml");
                                break;
                            case "Motovalle":
                            default:
                                await Html.RenderPartialAsync("~/Views/Shared/NavBars/_MotovalleNavBar.cshtml");
                                break;
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</nav><!--b-nav-->
<!-- End navigation pages -->