    <style type="text/css">
        @@keyframes slideInRight {
            from {
                transform: translate3d(100%, 0, 0);
                visibility: visible;
            }

            to {
                transform: translate3d(0, 0, 0);
            }
        }

        button.chatLauncher {
            animation-duration: 0.5s;
            transition-duration: 0.5s;
            position: fixed;
            bottom: 32px;
            left: 32px;
            z-index: 9999;
            border: 4px solid #0056CB;
            padding: 5px;
            border-radius: 0px;
            margin: 0;
            text-decoration: none;
            background-color: #0056CB;
            color: #ffffff;
            font-family: sans-serif;
            font-size: 0.8rem;
            cursor: pointer;
            text-align: left;
            -webkit-appearance: none;
            -moz-appearance: none;
            width: 185px;
            opacity: 0;
        }

        button.chatLauncher.open {
            animation-name: slideInRight;
            opacity: 1;
        }

        button.chatLauncher:hover,
        button.chatLauncher:focus {
            background-color: #E5E5E5;
            border: 4px solid #CDCDCD;
        }

        button.chatLauncher:focus {
            outline: 1px solid #CDCDCD;
            outline-offset: -4px;
        }

        .rejilla {
            display: grid;
            grid-template-columns: 45px 125px;
            align-items: start;
        }

        .chatElement {
            height: 600px;
            width: 100%;
        }
    </style>
</head>
