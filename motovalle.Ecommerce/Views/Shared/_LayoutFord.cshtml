            //recolección y asignación de variable que será enviada al asistete virtual
            element.querySelector('button').addEventListener('click', function addBackgroundColor(e) {
                datos = {
                    eCivil: element.querySelectorAll('select')[0].value,
                    sexo: element.querySelectorAll('input')[0].value,
                    ocupacion: element.querySelectorAll('select')[1].value,
                    contrato: element.querySelectorAll('select')[2].value,
                    vivienda: element.querySelectorAll('select')[3].value,
                    edad: element.querySelectorAll('input')[2].value,
                    ingresos: element.querySelectorAll('input')[3].value,
                    capacidad: element.querySelectorAll('input')[3].value - element.querySelectorAll('input')[4].value,
                    nPersonas: element.querySelectorAll('input')[5].value
                }
            });
            event.data.element.appendChild(element);
        }

        function TEMPORARY_convertToUserDefinedResponse(event) {
            if (event.data.output.user_defined && event.data.output.user_defined.template_name) {
                // Map over all items in the output array.
                event.data.output.generic = event.data.output.generic.map(function (item) {
                    // If we find one that matches our convention to transform to user_defined response type, make the transformation.
                    if (item.response_type === 'text' && item.text === 'replace with user_defined') {
                        item.response_type = 'user_defined';
                        item.user_defined = event.data.output.user_defined;
                        delete item.text;
                    }
                    return item;
                });
            }
        }


        function preSendhandler(event) {
            //Envío de datos al asistente
            event.data.context.skills['main skill'] = event.data.context.skills['main skill'] || { user_defined: {} };
            event.data.context.skills['main skill'].user_defined.resultado = datos;
        }


        function customResponseHandler(event) {
            //Manejo de evento y direccionamiento del evento
            const { message } = event.data;
            switch (message.user_defined.template_name) {
                case 'formulario':
                    handleFormularioTemplate(event);
                    break;
                default:
                    console.error('Unhandled response type.');
            }
        }
        const element = document.querySelector('.chatElement');
        window.watsonAssistantChatOptions = {

            integrationID: "537b9602-979a-4d3d-8ee7-a37a4aee3694",
            region: "us-south",
            serviceInstanceID: "24addc4c-175b-4230-a140-b1f1b54eb1b5",
            // Hide the default launcher.
            showLauncher: false,
            // Provide the custom element.
            element: element,

            onLoad: function (instance) {
                const button = document.querySelector('.chatLauncher');

                // Add the event listener to open your web chat.
                button.addEventListener('click', function clickListener() {
                    instance.openWindow();
                });

                instance.updateLocale('es').then(function () { instance.render().then(function () { button.style.display = 'block'; button.classList.add('open'); }); });

                // Temporary: Subscribe to the "pre:receive" event to convert to the user_defined response_type.
                instance.on({ type: 'pre:receive', handler: TEMPORARY_convertToUserDefinedResponse });

                // Subscribe to the "pre:send" event.
                instance.on({ type: "pre:send", handler: preSendhandler });

                // Watch for the customResponse event to handle the user defined response type.
                instance.on({ type: 'customResponse', handler: customResponseHandler });

            }
        };

        setTimeout(function () { const t = document.createElement('script'); t.src = 'https://web-chat.global.assistant.watson.appdomain.cloud/loadWatsonAssistantChat.js'; document.head.appendChild(t); });
    </script>

    @RenderSection("Scripts", required: false)
    <!-- End Scripts section -->
