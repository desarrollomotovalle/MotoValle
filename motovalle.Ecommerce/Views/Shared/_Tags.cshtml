@model IList<inventek_blog.Models.Entities.Tags>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="tags sidebar">
    <h3>Tags</h3>

    @if (Model.Count > 0)
    {
        foreach (var tag in Model)
        {
            @tag.TagName
        }
    }
    else
    {
        <p>No tags found!</p>
    }
</div>