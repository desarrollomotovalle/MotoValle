                        @foreach (var item in Model.ProductsAndMakeBase)
                        {
                            <div class="col-lg-4 col-md-4 col-xs-6" style="margin-bottom:2em;">
                                <div class="b-auto__main-item wow zoomInRight" data-wow-delay="0.5s">
