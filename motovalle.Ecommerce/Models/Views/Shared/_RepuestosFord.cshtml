        <div id="alternador" class="collapse">
            <div class="row">
                <div class="b-personal__worker-img">
                    <img src="~/repuestos/landing/alternador.jpg" class="rounded-lg" alt="Alternador" />
                </div>
            </div>
        </div>
    </div>

    <div class="b-compare__block wow zoomInUp">
        <button data-toggle="collapse" data-target="#motores" class="repuestos-button">
            <h4 class="pull-left"><b>MOTORES</b></h4> <span class="fa fa-angle-down fa-3x pull-right fordColor"></span>
        </button>
        <div id="motores" class="collapse">
            <div class="row">
                <div class="b-personal__worker-img">
                    <img src="~/repuestos/landing/motores.jpg" class="rounded-lg" alt="motores" />
                </div>
            </div>
        </div>
    </div>


    <div class="b-compare__block wow zoomInUp">
        <button data-toggle="collapse" data-target="#rejillas" class="repuestos-button">
            <h4 class="pull-left"><b>PARACHOQUES Y REJILLAS</b></h4> <span class="fa fa-angle-down fa-3x pull-right fordColor"></span>
        </button>
        <div id="rejillas" class="collapse">
            <div class="row">
                <div class="b-personal__worker-img">
                    <img src="~/repuestos/landing/parachoques.jpg" class="rounded-lg" alt="parachoques-rejillas" />
                </div>
            </div>
        </div>
    </div>
</div>

