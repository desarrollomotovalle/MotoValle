@using Microsoft.AspNetCore.Identity
@using motovalle.Ecommerce.Models.Entities.Identity

@inject UserManager<ApplicationUser> UserManager
@model MasseyFergusonBrandViewModel

@{
    ViewData["Title"] = "MasseyFerguson";
    Layout = "~/Views/Shared/_LayoutMasseyFerguson.cshtml";
    var userName = string.Empty;
    var userEmail = string.Empty;
    var userPhone = string.Empty;

    if (User.Identity.IsAuthenticated)
    {
        var user = await UserManager.GetUserAsync(User);
        userName = user.FullName;
        userEmail = user.Email;
        userPhone = user.PhoneNumber;
    }
}

<section class="b-pageHeader massey">
    <div class="container">
        <h1 class="wow zoomInLeft" data-wow-delay="0.5s">MOTOVALLE - MASSEY FERGUSON</h1>
        @*Issue 1: 04/08/2020 Diego - Remove Motovalle Park for Octavio's request*@
        @*<div class="b-pageHeader__search wow zoomInRight" data-wow-delay="0.5s">
                <h3>Encuentra la maquinaria que necesitas</h3>
            </div>*@
    </div>
</section><!--b-pageHeader-->

<div class="b-breadCumbs s-shadow wow zoomInUp" data-wow-delay="0.5s">
    <div class="container">
        <a asp-action="Index" asp-controller="Home" class="b-breadCumbs__page">Home</a><span class="fa fa-angle-right"></span><a asp-action="MasseyFerguson" asp-controller="Brands" class="b-breadCumbs__page m-active">Marca (Massey Ferguson)</a>
    </div>
    @if (ViewBag.Message != null)
    {
        <div class="alert alert-danger text-center wow zoomInUp" role="alert" data-wow-delay="0.5s">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <strong>¡Ups!..</strong> @ViewBag.Message &#128577;
        </div>
    }
</div><!--b-breadCumbs-->


<section class="b-slider">
    <div class="container wow zoomInRight" data-wow-delay="0.55s">
        <div id="carousel" class="slide carousel carousel-fade">
            <div class="carousel-inner">
                <div class="item active">
                    <img src="~/media/brand-sliders/massey-ferguson/header-1.jpg" alt="header-1" class="img-responsive" />
                </div>
                <div class="item">
                    <img src="~/media/brand-sliders/massey-ferguson/header-2.jpg" alt="header-2" class="img-responsive" />
                </div>
                <div class="item">
                    <img src="~/media/brand-sliders/massey-ferguson/header-3.jpg" alt="header-3" class="img-responsive" />
                </div>
                <div class="item">
                    <img src="~/media/brand-sliders/massey-ferguson/header-4.jpg" alt="header-4" class="img-responsive" />
                </div>
                <div class="item">
                    <img src="~/media/brand-sliders/massey-ferguson/header-5.jpg" alt="header-5" class="img-responsive" />
                </div>
            </div>
            <a class="carousel-control right" href="#carousel" data-slide="next" id="idNextSlider">
                <span class="fa fa-angle-right m-control-right"></span>
            </a>
            <a class="carousel-control left" href="#carousel" data-slide="prev">
                <span class="fa fa-angle-left m-control-left"></span>
            </a>
        </div>
    </div>
</section><!--b-slider-->




<div class="b-article s-shadow">
